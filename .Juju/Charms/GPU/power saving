Get(){
aticonfig --adapter=0 --od-getclocks
aticonfig --adapter=0 --od-gettemperature
}

Set(){
# Persist on reboot!  http://www.phoronix.com/forums/forum/linux-graphics-x-org-drivers/amd-linux/40054-cannot-overclock-card
# add to /etc/rc.local or Systemd equivalent

aticonfig --od-enable
aticonfig --od-setclocks 400,406
aticonfig --od-commitclocks
}


myTemperatures(){

Current Clocks :    400           406
Sensor 0: Temperature - 62.00 C



}
